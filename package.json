{
  "name": "spotifyCP",
  "description": "Spotify Crazy Playlist",
  "version": "0.1.0",
  "author": "delvallejonatan ",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint 'src/**/*.{js}'",
    "precommit": "npm run lint",
    "generate": "plop --plopfile generators/index.js",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "axios": "^0.21.1",
    "lscache": "1.1.0",
    "prop-types": "15.5.0",
    "query-string": "4.3.4",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "4.1.1",
    "recompose": "0.23.4",
    "redux": "^4.0.5",
    "redux-actions": "2.0.3",
    "redux-thunk": "2.2.0",
    "reselect": "3.0.1",
    "styled-components": "2.0.0",
    "styled-components-theme": "1.0.5"
  },
  "devDependencies": {
    "babel-eslint": "7.2.3",
    "enzyme": "2.8.2",
    "enzyme-to-json": "1.5.1",
    "eslint": "3.19.0",
    "eslint-config-react-app": "1.0.1",
    "eslint-plugin-flowtype": "2.33.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "5.0.1",
    "eslint-plugin-react": "7.0.1",
    "fetch-mock": "5.11.0",
    "gh-pages": "1.0.0",
    "husky": "0.13.3",
    "jest": "20.0.4",
    "node-plop": "0.5.5",
    "plop": "1.7.4",
    "react-addons-test-utils": "15.6.0",
    "react-scripts": "1.0.2",
    "redux-devtools-extension": "2.13.0"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "react/jsx-space-before-closing": [
        2,
        "always"
      ],
      "react/jsx-closing-bracket-location": [
        2,
        "tag-aligned"
      ],
      "semi": [
        "error",
        "never"
      ],
      "max-len": [
        "error",
        130
      ]
    }
  },
  "engines": {
    "npm": ">=4",
    "node": ">=6"
  },
  "homepage": "https://jonidelv.github.io/spotifyCP"
}
